                    RowBox[{"Im", "[", "\[Sigma]", "]"}], "t"}], "]"}]}]}], 
                    ")"}], 
                    RowBox[{"Exp", "[", 
                    RowBox[{
                    RowBox[{"Re", "[", "\[Sigma]", "]"}], "t"}], "]"}]}]}], 
                  ")"}]}], "/.", "buffer"}], "/.", 
               RowBox[{"fluidPairing", "[", "name", "]"}]}], "/.", 
              "wavenumber"}], "//", "Re"}], "//", "ComplexExpand"}], "//", 
           "Simplify"}], "//", "First"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"expression", "[", "name", "]"}], "=", 
