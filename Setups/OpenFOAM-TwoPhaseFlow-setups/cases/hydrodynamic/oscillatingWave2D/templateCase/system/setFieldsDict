FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.dispersed 0
);

regions
(
    fieldToCell
    {
        field   inputField;
        min     0.5;
        max     1.0;
        fieldValues
        (
            volScalarFieldValue alpha.dispersed 1
        );
    }
);


